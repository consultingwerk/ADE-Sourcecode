  TABLE-TRIGGER "CREATE" OVERRIDE PROCEDURE "icf/trg/rycuetrigc.p" CRC "?" 
  TABLE-TRIGGER "DELETE" OVERRIDE PROCEDURE "icf/trg/rycuetrigd.p" CRC "?" 
