  FORMAT "->>>>>>>>>>>>>>>>>9.999999999"
  INITIAL "0"
  LABEL "Object type obj"
  POSITION 2
  MAX-WIDTH 24
  VALMSG "o_obj"
  HELP "Object type obj"
