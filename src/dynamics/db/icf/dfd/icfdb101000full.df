  TABLE-TRIGGER "CREATE" OVERRIDE PROCEDURE "icf/trg/gsccptrigc.p" CRC "?" 
  TABLE-TRIGGER "DELETE" OVERRIDE PROCEDURE "icf/trg/gsccptrigd.p" CRC "?" 
