&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CUSTOM _MAIN-BLOCK Procedure 


/* ***************************  Main Block  *************************** */

DEFINE INPUT PARAMETER pcObjectName               AS CHARACTER  NO-UNDO.
DEFINE OUTPUT PARAMETER pdObject                  AS DECIMAL    NO-UNDO.

FIND FIRST ryc_smartobject NO-LOCK
     WHERE ryc_smartobject.OBJECT_filename BEGINS pcObjectName
     NO-ERROR.

IF AVAILABLE ryc_smartobject THEN
  ASSIGN pdObject = ryc_smartobject.smartobject_obj.
ELSE
  ASSIGN pdObject = 0.

ERROR-STATUS:ERROR = NO.
RETURN.

