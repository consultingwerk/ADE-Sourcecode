    qbf-lang[ 8] = 'd,Το Μήνα ως αριθμός,n00=d28,MONTH(~{1~})'
    qbf-lang[ 9] = 'd,Η Ονομασία του Μήνα,s00=d29,ENTRY(MONTH(~{1~});"Ιανουάριος'
