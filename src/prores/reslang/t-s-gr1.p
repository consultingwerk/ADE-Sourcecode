    qbf-lang[ 3] = 's,Συνδιασμός δύο Αλφαριθμητικών,s00=s28s29,~{1~} + ~{2~}'
    qbf-lang[ 4] = 's,Συνδιασμός τριών Αλφαριθμητικών,s00=s28s29s29,'
