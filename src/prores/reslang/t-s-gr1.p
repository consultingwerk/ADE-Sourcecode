    qbf-lang[ 2] = 'Βοήθεια'

/*s-order.p*/
    qbf-lang[15] = 'Αύξουσα/Φθίνουσα' /*neither can be over 8 characters */
    qbf-lang[16] = 'Για το καθένα δώστε "Α" για αύξουσα σειρά'
    qbf-lang[17] = 'ή "Φ" για φθίνουσα σειρά.'

