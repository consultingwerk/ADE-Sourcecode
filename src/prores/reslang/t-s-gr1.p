    qbf-lang[ 5] = 's,Συνδιασμός τεσσάρων Αλφαριθμητικών,s00=s28s29s29s29,'
		 + '~{1~} + ~{2~} + ~{3~} + ~{4~}'
    qbf-lang[ 6] = 's,Το μικρότερο δύο Αλφαριθμητικών,s00=s30s31,MINIMUM(~{1~};~{2~})'
    qbf-lang[ 7] = 's,Το μεγαλύτερο δύο Αλφαριθμητικών,s00=s30s31,MAXIMUM(~{1~};~{2~})'
