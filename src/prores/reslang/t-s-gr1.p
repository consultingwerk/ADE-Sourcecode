/*--------------------------------------------------------------------------*/

/* s-calc.p - mathematical expressions */
IF qbf-s = 8 THEN
  ASSIGN
    qbf-lang[ 1] = 'm,Πρόσθεση,n00=n25n26m...,~{1~} + ~{2~}'
    qbf-lang[ 2] = 'm,Αφαίρεση,n00=n25n27m...,~{1~} - ~{2~}'
