  ASSIGN
    qbf-lang[ 1] = 'n,Σταθερό ή Πεδίο,n00=n26,~{1~}'
    qbf-lang[ 2] = 'n,Το Μικρότερο δύο αριθμών,n00=n24n25,MINIMUM(~{1~};~{2~})'
